    "bcrypt": "^0.8.7",
    "body-parser": "~1.8.1",
    "bson": "^0.4.20",
    "cookie-parser": "~1.3.3",
    "css-loader": "^0.23.0",
    "debug": "~2.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^5.2.2",
    "express": "~4.9.0",
    "express-session": "^1.11.3",
    "hashids": "^1.0.2",
    "jade": "~1.6.0",
    "jsonwebtoken": "^7.1.6",
