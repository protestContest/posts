{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -f public/dist/* & rm -rf components/scripts/dist",
    "prelint": "npm -s run clean",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "prebuild": "npm -s run lint",
    "build": "npm -s run build:js",
    "prebuild:js": "npm -s run build:jsx",
    "build:js": "webpack -d components/scripts/dist/*.js public/dist/components.js",
    "build:jsx": "mkdir -p components/scripts/dist && ./node_modules/.bin/babel components/scripts/src --out-dir components/scripts/dist",
    "prestart": "npm -s run build",
    "start": "node ./bin/www",
    "prestart:debug": "npm -s run build",
    "start:debug": "node debug ./bin/www",
    "poststart:debug": "npm -s run clean",
    "pretest": "npm -s run build",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha",
    "pretest:watch": "npm -s run build",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/mocha -w -R min"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "jade": "~1.6.0",
    "markdown": "^0.5.0",
    "mongoose": "^3.8.18",
    "morgan": "~1.3.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "serve-favicon": "~2.1.3",
    "slug": "^0.8.0"
  },
  "engines": {
    "node": "0.12.x"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "clean-css": "^3.4.8",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.8.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "watch": "^0.16.0",
    "webpack": "^1.12.6"
  }
}
