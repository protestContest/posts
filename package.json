{
  "name": "app",
  "version": "1.0.12",
  "private": true,
  "scripts": {
    "lint": "eslint app/src lib models routes test server.js --ext .js --ext .jsx",
    "prebuild": "npm -s run lint",
    "build": "webpack",
    "prestart": "npm -s run build",
    "start": "node ./bin/www",
    "debug": "node debug ./bin/www",
    "pretest": "npm -s run build",
    "test": "npm run test:unit && npm run test:functional",
    "test:watch": "NODE_ENV=test mocha -w -R min",
    "test:unit": "NODE_ENV=test mocha test/unit",
    "test:functional": "NODE_ENV=test mocha test/func"
  },
