{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -f public/scripts/*.js public/stylesheets/*.css",
    "prelint": "npm -s run clean",
    "lint": "./node_modules/.bin/eslint .",
    "prebuild": "npm -s run lint",
    "build": "npm -s run build:js && npm -s run build:css",
    "build:css": "for f in public/stylesheets/components/*.less; do ./node_modules/.bin/lessc $f; done | ./node_modules/.bin/cleancss > public/stylesheets/style.css",
    "build:js": "./node_modules/.bin/babel public/scripts/components/*.jsx > public/scripts/components.js",
    "prestart": "npm -s run build",
    "start": "node ./bin/www",
    "pretest": "npm -s run build",
    "test": "./node_modules/.bin/mocha",
    "test:watch": "./node_modules/.bin/mocha -w"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "express-session": "^1.11.3",
    "jade": "~1.6.0",
    "markdown": "^0.5.0",
    "mongoose": "^3.8.18",
    "morgan": "~1.3.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "serve-favicon": "~2.1.3",
    "slug": "^0.8.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "eslint-plugin-react": "^3.8.0",
    "mocha": "^2.3.3",
    "supertest": "^1.1.0"
  }
}
